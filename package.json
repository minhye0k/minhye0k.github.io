    "gatsby-plugin-mdx": "^5.10.0",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sitemap": "^6.9.0",
    "gatsby-plugin-webpack-statoscope": "^1.0.2",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.3",
    "@types/prismjs": "^1.26.0",
